{
  "name": "WindowNamer",
  "description":"Name all tabs in a window",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_title": "Window Namer",
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}